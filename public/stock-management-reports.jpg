<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f0f4f8"/>
  <rect width="700" height="500" x="50" y="50" rx="10" fill="#ffffff" stroke="#2563EB" stroke-width="2"/>
  
  <!-- Header -->
  <rect width="700" height="60" x="50" y="50" rx="10" fill="#2563EB" opacity="0.1"/>
  <text x="80" y="85" font-family="Arial" font-size="18" font-weight="bold" fill="#2563EB">Reportes de Inventario</text>
  
  <!-- Report Selector -->
  <rect width="660" height="60" x="70" y="120" rx="5" fill="#f3f4f6"/>
  <text x="90" y="155" font-family="Arial" font-size="14" fill="#4b5563">Tipo de Reporte:</text>
  
  <rect width="140" height="35" x="220" y="132" rx="5" fill="#2563EB"/>
  <text x="290" y="155" font-family="Arial" font-size="12" text-anchor="middle" fill="white">Inventario Valorizado</text>
  
  <rect width="140" height="35" x="370" y="132" rx="5" fill="white" stroke="#2563EB" stroke-width="1"/>
  <text x="440" y="155" font-family="Arial" font-size="12" text-anchor="middle" fill="#2563EB">Rotación de Stock</text>
  
  <rect width="140" height="35" x="520" y="132" rx="5" fill="white" stroke="#2563EB" stroke-width="1"/>
  <text x="590" y="155" font-family="Arial" font-size="12" text-anchor="middle" fill="#2563EB">Alertas de Stock</text>
  
  <!-- Date range selector -->
  <rect width="200" height="35" x="530" y="190" rx="5" fill="white" stroke="#2563EB" stroke-width="1"/>
  <text x="570" y="212" font-family="Arial" font-size="12" fill="#4b5563">01/04/2025 - 30/04/2025</text>
  <text x="720" y="212" font-family="Arial" font-size="12" fill="#2563EB">▼</text>
  
  <!-- Chart area -->
  <rect width="660" height="250" x="70" y="230" rx="5" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <text x="400" y="260" font-family="Arial" font-size="16" font-weight="bold" text-anchor="middle" fill="#374151">Valor de Inventario por Categoría (en miles de $)</text>
  
  <!-- Bar chart -->
  <rect width="100" height="150" x="130" y="290" fill="#2563EB" opacity="0.8"/>
  <text x="180" y="455" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">Ropa</text>
  <text x="180" y="280" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">$1,250</text>
  
  <rect width="100" height="100" x="260" y="340" fill="#2563EB" opacity="0.6"/>
  <text x="310" y="455" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">Calzado</text>
  <text x="310" y="330" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">$785</text>
  
  <rect width="100" height="65" x="390" y="375" fill="#2563EB" opacity="0.7"/>
  <text x="440" y="455" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">Accesorios</text>
  <text x="440" y="365" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">$520</text>
  
  <rect width="100" height="120" x="520" y="320" fill="#2563EB" opacity="0.5"/>
  <text x="570" y="455" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">Otros</text>
  <text x="570" y="310" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">$950</text>
  
  <!-- Export options -->
  <rect width="220" height="120" x="510" y="490" rx="5" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <text x="620" y="515" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="#374151">Exportar</text>
  
  <rect width="180" height="30" x="530" y="525" rx="4" fill="#f3f4f6"/>
  <text x="620" y="545" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">PDF</text>
  
  <rect width="180" height="30" x="530" y="565" rx="4" fill="#f3f4f6"/>
  <text x="620" y="585" font-family="Arial" font-size="12" text-anchor="middle" fill="#4b5563">Excel (.xlsx)</text>
  
  <!-- Summary statistics -->
  <rect width="420" height="120" x="70" y="490" rx="5" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <text x="90" y="515" font-family="Arial" font-size="14" font-weight="bold" fill="#374151">Resumen</text>
  
  <text x="90" y="545" font-family="Arial" font-size="12" fill="#4b5563">Total Valorizado:</text>
  <text x="230" y="545" font-family="Arial" font-size="12" font-weight="bold" fill="#2563EB">$3,505,000</text>
  
  <text x="90" y="570" font-family="Arial" font-size="12" fill="#4b5563">Crecimiento vs Mes Anterior:</text>
  <text x="230" y="570" font-family="Arial" font-size="12" font-weight="bold" fill="#10b981">+8.5%</text>
  
  <text x="90" y="595" font-family="Arial" font-size="12" fill="#4b5563">Sucursal con Mayor Inventario:</text>
  <text x="230" y="595" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Centro (45%)</text>
</svg>