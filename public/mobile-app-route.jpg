<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f0f4f8"/>
  
  <!-- Smartphone frame -->
  <rect width="300" height="560" x="250" y="20" rx="30" fill="#374151"/>
  <rect width="280" height="520" x="260" y="40" rx="20" fill="#f3f4f6"/>
  
  <!-- Mobile app header -->
  <rect width="280" height="60" x="260" y="40" rx="20" fill="#2563EB"/>
  <text x="290" y="75" font-family="Arial" font-size="16" font-weight="bold" fill="white">Planificación de Ruta</text>
  
  <!-- Back button -->
  <circle cx="280" cy="70" r="15" fill="white" opacity="0.2"/>
  <text x="280" y="75" font-family="Arial" font-size="18" font-weight="bold" text-anchor="middle" fill="white">←</text>
  
  <!-- Date selector -->
  <rect width="240" height="40" x="280" y="110" rx="20" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <text x="310" y="135" font-family="Arial" font-size="13" fill="#4b5563">Hoy: 20/05/2025</text>
  <text x="500" y="135" font-family="Arial" font-size="13" font-weight="bold" text-anchor="end" fill="#2563EB">▼</text>
  
  <!-- Map view -->
  <rect width="240" height="180" x="280" y="160" rx="10" fill="#e5e7eb"/>
  
  <!-- Map elements -->
  <!-- Roads -->
  <line x1="300" y1="200" x2="500" y2="200" stroke="white" stroke-width="5"/>
  <line x1="300" y1="250" x2="500" y2="250" stroke="white" stroke-width="5"/>
  <line x1="300" y1="300" x2="500" y2="300" stroke="white" stroke-width="5"/>
  <line x1="350" y1="170" x2="350" y2="330" stroke="white" stroke-width="5"/>
  <line x1="400" y1="170" x2="400" y2="330" stroke="white" stroke-width="5"/>
  <line x1="450" y1="170" x2="450" y2="330" stroke="white" stroke-width="5"/>
  
  <!-- Locations -->
  <circle cx="350" cy="200" r="8" fill="#ef4444"/>
  <text x="350" y="200" font-family="Arial" font-size="10" font-weight="bold" text-anchor="middle" fill="white">1</text>
  
  <circle cx="400" cy="250" r="8" fill="#ef4444"/>
  <text x="400" y="250" font-family="Arial" font-size="10" font-weight="bold" text-anchor="middle" fill="white">2</text>
  
  <circle cx="450" cy="200" r="8" fill="#ef4444"/>
  <text x="450" y="200" font-family="Arial" font-size="10" font-weight="bold" text-anchor="middle" fill="white">3</text>
  
  <circle cx="350" cy="300" r="8" fill="#ef4444"/>
  <text x="350" y="300" font-family="Arial" font-size="10" font-weight="bold" text-anchor="middle" fill="white">4</text>
  
  <circle cx="450" cy="300" r="8" fill="#10b981"/>
  <text x="450" y="300" font-family="Arial" font-size="10" font-weight="bold" text-anchor="middle" fill="white">✓</text>
  
  <!-- Route -->
  <line x1="350" y1="200" x2="400" y2="250" stroke="#2563EB" stroke-width="3" stroke-dasharray="5,5"/>
  <line x1="400" y1="250" x2="450" y2="200" stroke="#2563EB" stroke-width="3" stroke-dasharray="5,5"/>
  <line x1="450" y1="200" x2="350" y2="300" stroke="#2563EB" stroke-width="3" stroke-dasharray="5,5"/>
  <line x1="350" y1="300" x2="450" y2="300" stroke="#2563EB" stroke-width="3" stroke-dasharray="5,5"/>
  
  <!-- Client list -->
  <text x="280" y="360" font-family="Arial" font-size="14" font-weight="bold" fill="#374151">Clientes para Hoy:</text>
  
  <!-- Client 1 -->
  <rect width="240" height="60" x="280" y="370" rx="10" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <circle cx="300" cy="400" r="15" fill="#ef4444" opacity="0.2"/>
  <text x="300" y="404" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle" fill="#ef4444">1</text>
  <text x="330" y="390" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Comercial Mitre SRL</text>
  <text x="330" y="410" font-family="Arial" font-size="11" fill="#4b5563">09:30 - Av. Rivadavia 1234</text>
  <rect width="40" height="20" x="480" y="390" rx="10" fill="#2563EB" opacity="0.1"/>
  <text x="500" y="403" font-family="Arial" font-size="10" text-anchor="middle" fill="#2563EB">Ver</text>
  
  <!-- Client 2 -->
  <rect width="240" height="60" x="280" y="440" rx="10" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <circle cx="300" cy="470" r="15" fill="#ef4444" opacity="0.2"/>
  <text x="300" y="474" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle" fill="#ef4444">2</text>
  <text x="330" y="460" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Textiles Del Sur</text>
  <text x="330" y="480" font-family="Arial" font-size="11" fill="#4b5563">11:00 - Av. Corrientes 567</text>
  <rect width="40" height="20" x="480" y="460" rx="10" fill="#2563EB" opacity="0.1"/>
  <text x="500" y="473" font-family="Arial" font-size="10" text-anchor="middle" fill="#2563EB">Ver</text>
  
  <!-- Client 3 -->
  <rect width="240" height="60" x="280" y="510" rx="10" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <circle cx="300" cy="540" r="15" fill="#ef4444" opacity="0.2"/>
  <text x="300" y="544" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle" fill="#ef4444">3</text>
  <text x="330" y="530" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Dist. Mendoza SRL</text>
  <text x="330" y="550" font-family="Arial" font-size="11" fill="#4b5563">14:30 - Av. Santa Fe 890</text>
  <rect width="40" height="20" x="480" y="530" rx="10" fill="#2563EB" opacity="0.1"/>
  <text x="500" y="543" font-family="Arial" font-size="10" text-anchor="middle" fill="#2563EB">Ver</text>
  
  <!-- App features description -->
  <rect width="200" height="280" x="50" y="150" rx="10" fill="white" stroke="#2563EB" stroke-width="2"/>
  <text x="70" y="180" font-family="Arial" font-size="16" font-weight="bold" fill="#2563EB">Geolocalización:</text>
  
  <text x="70" y="215" font-family="Arial" font-size="14" fill="#4b5563">• Mapa interactivo</text>
  <text x="70" y="240" font-family="Arial" font-size="14" fill="#4b5563">• Clientes geolocalizados</text>
  <text x="70" y="265" font-family="Arial" font-size="14" fill="#4b5563">• Planificación de rutas</text>
  <text x="70" y="290" font-family="Arial" font-size="14" fill="#4b5563">• Optimización de</text>
  <text x="85" y="310" font-family="Arial" font-size="14" fill="#4b5563">recorridos</text>
  <text x="70" y="335" font-family="Arial" font-size="14" fill="#4b5563">• Navegación paso a paso</text>
  <text x="70" y="360" font-family="Arial" font-size="14" fill="#4b5563">• Cálculo de tiempos</text>
  <text x="70" y="385" font-family="Arial" font-size="14" fill="#4b5563">• Búsqueda de clientes</text>
  <text x="85" y="405" font-family="Arial" font-size="14" fill="#4b5563">cercanos</text>
  <text x="70" y="430" font-family="Arial" font-size="14" fill="#4b5563">• Registro de visitas</text>
  
  <rect width="200" height="280" x="550" y="150" rx="10" fill="white" stroke="#2563EB" stroke-width="2"/>
  <text x="570" y="180" font-family="Arial" font-size="16" font-weight="bold" fill="#2563EB">Beneficios:</text>
  
  <text x="570" y="215" font-family="Arial" font-size="14" fill="#4b5563">• Optimización de</text>
  <text x="585" y="235" font-family="Arial" font-size="14" fill="#4b5563">tiempos</text>
  <text x="570" y="260" font-family="Arial" font-size="14" fill="#4b5563">• Reducción de costos</text>
  <text x="585" y="280" font-family="Arial" font-size="14" fill="#4b5563">de transporte</text>
  <text x="570" y="305" font-family="Arial" font-size="14" fill="#4b5563">• Mayor cantidad de</text>
  <text x="585" y="325" font-family="Arial" font-size="14" fill="#4b5563">visitas por día</text>
  <text x="570" y="350" font-family="Arial" font-size="14" fill="#4b5563">• Control de trabajo</text>
  <text x="585" y="370" font-family="Arial" font-size="14" fill="#4b5563">en campo</text>
  <text x="570" y="395" font-family="Arial" font-size="14" fill="#4b5563">• Estadísticas de</text>
  <text x="585" y="415" font-family="Arial" font-size="14" fill="#4b5563">tiempo por cliente</text>
</svg>