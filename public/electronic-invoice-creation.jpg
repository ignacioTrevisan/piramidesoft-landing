<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f0f4f8"/>
  <rect width="700" height="500" x="50" y="50" rx="10" fill="#ffffff" stroke="#2563EB" stroke-width="2"/>
  
  <!-- Header -->
  <rect width="700" height="60" x="50" y="50" rx="10" fill="#2563EB" opacity="0.1"/>
  <text x="80" y="85" font-family="Arial" font-size="18" font-weight="bold" fill="#2563EB">Creación de Factura Electrónica</text>
  
  <!-- Main form area -->
  <rect width="660" height="420" x="70" y="120" rx="5" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  
  <!-- Client info section -->
  <text x="90" y="150" font-family="Arial" font-size="14" font-weight="bold" fill="#374151">Datos del Cliente</text>
  
  <text x="90" y="180" font-family="Arial" font-size="12" fill="#4b5563">Tipo:</text>
  <rect width="140" height="30" x="130" y="160" rx="5" fill="#f3f4f6"/>
  <text x="145" y="180" font-family="Arial" font-size="12" fill="#4b5563">Consumidor Final</text>
  <text x="260" y="180" font-family="Arial" font-size="12" fill="#4b5563">▼</text>
  
  <text x="290" y="180" font-family="Arial" font-size="12" fill="#4b5563">CUIT/DNI:</text>
  <rect width="150" height="30" x="360" y="160" rx="5" fill="#f3f4f6"/>
  <text x="375" y="180" font-family="Arial" font-size="12" fill="#4b5563">20-12345678-9</text>
  
  <text x="530" y="180" font-family="Arial" font-size="12" fill="#4b5563">Condición IVA:</text>
  <rect width="110" height="30" x="620" y="160" rx="5" fill="#f3f4f6"/>
  <text x="635" y="180" font-family="Arial" font-size="12" fill="#4b5563">Resp. Inscripto</text>
  
  <text x="90" y="220" font-family="Arial" font-size="12" fill="#4b5563">Razón Social:</text>
  <rect width="300" height="30" x="170" y="200" rx="5" fill="#f3f4f6"/>
  <text x="185" y="220" font-family="Arial" font-size="12" fill="#4b5563">Comercial Rodriguez S.A.</text>
  
  <text x="490" y="220" font-family="Arial" font-size="12" fill="#4b5563">Email:</text>
  <rect width="240" height="30" x="540" y="200" rx="5" fill="#f3f4f6"/>
  <text x="555" y="220" font-family="Arial" font-size="12" fill="#4b5563">contacto@rodriguez.com.ar</text>
  
  <!-- Invoice details section -->
  <line x1="70" y1="245" x2="730" y2="245" stroke="#e5e7eb" stroke-width="1"/>
  <text x="90" y="270" font-family="Arial" font-size="14" font-weight="bold" fill="#374151">Datos del Comprobante</text>
  
  <text x="90" y="300" font-family="Arial" font-size="12" fill="#4b5563">Tipo:</text>
  <rect width="120" height="30" x="130" y="280" rx="5" fill="#f3f4f6"/>
  <text x="145" y="300" font-family="Arial" font-size="12" fill="#4b5563">Factura A</text>
  <text x="240" y="300" font-family="Arial" font-size="12" fill="#4b5563">▼</text>
  
  <text x="270" y="300" font-family="Arial" font-size="12" fill="#4b5563">Punto de Venta:</text>
  <rect width="80" height="30" x="370" y="280" rx="5" fill="#f3f4f6"/>
  <text x="385" y="300" font-family="Arial" font-size="12" fill="#4b5563">00001</text>
  <text x="440" y="300" font-family="Arial" font-size="12" fill="#4b5563">▼</text>
  
  <text x="470" y="300" font-family="Arial" font-size="12" fill="#4b5563">Fecha:</text>
  <rect width="110" height="30" x="520" y="280" rx="5" fill="#f3f4f6"/>
  <text x="535" y="300" font-family="Arial" font-size="12" fill="#4b5563">15/05/2025</text>
  
  <text x="650" y="300" font-family="Arial" font-size="12" fill="#4b5563">N°:</text>
  <rect width="80" height="30" x="670" y="280" rx="5" fill="#f3f4f6"/>
  <text x="685" y="300" font-family="Arial" font-size="12" fill="#4b5563">00000123</text>
  
  <!-- Items section -->
  <line x1="70" y1="325" x2="730" y2="325" stroke="#e5e7eb" stroke-width="1"/>
  <text x="90" y="350" font-family="Arial" font-size="14" font-weight="bold" fill="#374151">Detalle de Items</text>
  
  <!-- Table header -->
  <rect width="640" height="30" x="90" y="365" fill="#f3f4f6"/>
  <text x="110" y="385" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Código</text>
  <text x="220" y="385" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Descripción</text>
  <text x="420" y="385" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Cantidad</text>
  <text x="500" y="385" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Precio Unit.</text>
  <text x="600" y="385" font-family="Arial" font-size="12" font-weight="bold" fill="#374151">Subtotal</text>
  <line x1="90" y1="395" x2="730" y2="395" stroke="#e5e7eb" stroke-width="1"/>
  
  <!-- Row 1 -->
  <text x="110" y="420" font-family="Arial" font-size="12" fill="#4b5563">RP001</text>
  <text x="220" y="420" font-family="Arial" font-size="12" fill="#4b5563">Camiseta Algodón Premium</text>
  <text x="420" y="420" font-family="Arial" font-size="12" text-anchor="center" fill="#4b5563">5</text>
  <text x="500" y="420" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$3,500.00</text>
  <text x="600" y="420" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$17,500.00</text>
  <text x="700" y="420" font-family="Arial" font-size="12" text-anchor="center" fill="#ef4444">×</text>
  
  <!-- Row 2 -->
  <line x1="90" y1="430" x2="730" y2="430" stroke="#e5e7eb" stroke-width="1"/>
  <text x="110" y="455" font-family="Arial" font-size="12" fill="#4b5563">CZ001</text>
  <text x="220" y="455" font-family="Arial" font-size="12" fill="#4b5563">Zapatillas Running Profesional</text>
  <text x="420" y="455" font-family="Arial" font-size="12" text-anchor="center" fill="#4b5563">1</text>
  <text x="500" y="455" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$12,500.00</text>
  <text x="600" y="455" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$12,500.00</text>
  <text x="700" y="455" font-family="Arial" font-size="12" text-anchor="center" fill="#ef4444">×</text>
  
  <!-- Row 3 -->
  <line x1="90" y1="465" x2="730" y2="465" stroke="#e5e7eb" stroke-width="1"/>
  <text x="110" y="490" font-family="Arial" font-size="12" fill="#4b5563">AC001</text>
  <text x="220" y="490" font-family="Arial" font-size="12" fill="#4b5563">Reloj Deportivo Multifunción</text>
  <text x="420" y="490" font-family="Arial" font-size="12" text-anchor="center" fill="#4b5563">2</text>
  <text x="500" y="490" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$8,750.00</text>
  <text x="600" y="490" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$17,500.00</text>
  <text x="700" y="490" font-family="Arial" font-size="12" text-anchor="center" fill="#ef4444">×</text>
  
  <!-- Add product -->
  <line x1="90" y1="500" x2="730" y2="500" stroke="#e5e7eb" stroke-width="1"/>
  <rect width="120" height="30" x="90" y="510" rx="5" fill="#2563EB" opacity="0.1"/>
  <text x="150" y="530" font-family="Arial" font-size="12" text-anchor="middle" fill="#2563EB">+ Agregar Item</text>
  
  <!-- Totals -->
  <rect width="250" height="120" x="480" y="510" fill="white" stroke="#e5e7eb" stroke-width="1"/>
  <text x="500" y="535" font-family="Arial" font-size="12" fill="#4b5563">Subtotal:</text>
  <text x="710" y="535" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$47,500.00</text>
  
  <text x="500" y="560" font-family="Arial" font-size="12" fill="#4b5563">IVA (21%):</text>
  <text x="710" y="560" font-family="Arial" font-size="12" text-anchor="end" fill="#4b5563">$9,975.00</text>
  
  <line x1="500" y1="570" x2="710" y2="570" stroke="#e5e7eb" stroke-width="1"/>
  
  <text x="500" y="595" font-family="Arial" font-size="14" font-weight="bold" fill="#4b5563">TOTAL:</text>
  <text x="710" y="595" font-family="Arial" font-size="14" font-weight="bold" text-anchor="end" fill="#2563EB">$57,475.00</text>
  
  <!-- Action buttons -->
  <rect width="660" height="50" x="70" y="550" rx="5" fill="#f9fafb"/>
  
  <rect width="140" height="35" x="90" y="558" rx="5" fill="white" stroke="#2563EB" stroke-width="1"/>
  <text x="160" y="580" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle" fill="#2563EB">Vista Previa</text>
  
  <rect width="140" height="35" x="240" y="558" rx="5" fill="#2563EB"/>
  <text x="310" y="580" font-family="Arial" font-size="12" font-weight="bold" text-anchor="middle" fill="white">Emitir Factura</text>
</svg>